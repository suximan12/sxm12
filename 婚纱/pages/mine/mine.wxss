/* pages/mine/mine.wxss */
.mine_tou{
  width: 100%;
  height: 350rpx;
  background-color: #D05746;
}
.mine_tou navigator:nth-child(1){
  font-size: 28rpx;
  color: #fff;
  float: left;
  margin-left: 30rpx;
}
.mine_tou navigator:nth-child(2){
  float: right;
  font-size: 28rpx;
  color: #fff;
  margin-right: 30rpx;
}
.mine_tou navigator:nth-child(3){
  /* width: 120rpx; */
  height: 400rpx;
  color: #fff;
  margin-left: 30%;
}
.mine_tou image{
  width: 150rpx;
  height: 150rpx;
  border-radius: 75rpx;
  margin-top: 60rpx;
  margin-left: 20%;
}
.mine_tou text{
  position: absolute;
  margin-top: -180rpx;
  margin-left: 45%;
}
.mine_second{
  width: 90%;
  height: 200rpx;
  margin: 0 auto;
  margin-top: -60rpx;
  border-radius: 30rpx;
  background-color: #F6F6F6;
  box-shadow: 10rpx 10rpx 10rpx #e3e3e3;
}
.mine_second_img{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.mine_second_img image{
  width: 90rpx;
  height: 80rpx;
  margin-top: 10rpx;
}
.mine_second_nav{
  font-size: 28rpx;
  margin-top: 10rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.mine_second_nav navigator:nth-child(2) {
  margin-left: 30rpx;
}
.mine_second_nav navigator:nth-child(3) {
  margin-left: 25rpx;
}
.mine_second_text{
  font-size: 28rpx;
  margin-top: 10rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.mine_third{
  width: 90%;
  height: 400rpx;
  margin: 0 auto;
  margin-top: 30rpx;
  font-size: 32rpx;
  border-radius: 30rpx;
  background-color: #F6F6F6;
  box-shadow: 10rpx 10rpx 10rpx #e3e3e3;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.mine_third_d{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.mine_third_d text:nth-child(1){
  margin-left: 30rpx;
}
.mine_third_d text:nth-child(2){
  margin-right: 30rpx;
}
.mine_fourth{
  width: 86%;
  height: 130rpx;
  margin: 0 auto;
  margin-top: 30rpx;
  font-size: 28rpx;
  border-radius: 30rpx;
  background-color: #F6F6F6;
  box-shadow: 10rpx 10rpx 10rpx #e3e3e3;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  padding-left: 30rpx;
}
